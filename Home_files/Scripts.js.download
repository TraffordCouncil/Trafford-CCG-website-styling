(function ($) {
	$(document).ready(function () {
		$('.forms-dropdown').on('change', null, function () {
			var url = $(this).val();
			if (url) {
				ga('send', 'event', {
					eventCategory: 'internal',
					eventAction: 'from ' + $(this).attr('id'),
					eventLabel: url
				});
				$(this).prop('selectedIndex', 0);
				window.open(url);
			}
			return false;
		});
	});
})(jQuery);